{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": ["warn", { 
      "varsIgnorePattern": "^_",
      "argsIgnorePattern": "^_"
    }],
    "no-console": "off"
  }
}
